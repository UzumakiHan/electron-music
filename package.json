{
  "name": "UzumakiHan-music",
  "version": "2.0.1",
  "main": "dist-electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview",
    "electron:generate-icons": "electron-icon-builder --input=./public/logo.jpg --output=build --flatten"
  },
  "devDependencies": {
    "@types/qs": "^6.9.7",
    "@types/swiper": "^6.0.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "electron": "^25.0.1",
    "electron-builder": "^24.4.0",
    "electron-icon-builder": "^2.0.1",
    "less": "^4.1.3",
    "typescript": "^5.0.2",
    "unocss": "^0.54.1",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.12.0",
    "vite-plugin-electron-renderer": "^0.14.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "NeteaseCloudMusicApi": "^4.9.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "element-plus": "^2.3.9",
    "lodash": "^4.17.21",
    "pinia": "^2.1.6",
    "qs": "^6.11.2",
    "swiper": "^10.3.1",
    "vue-router": "^4.2.4",
    "vue3-video-play": "1.3.1-beta.6",
    "vuex": "^4.1.0"
  }
}
